<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.order.mapper.OrderItemMapper">

    <resultMap id="BaseResultMap" type="com.cloud.order.module.entity.OrderItem">
        <id property="id" column="id"/>
        <result property="orderId" column="order_id"/>
        <result property="productId" column="product_id"/>
        <result property="productSnapshot" column="product_snapshot"/>
        <result property="quantity" column="quantity"/>
        <result property="price" column="price"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,order_id,product_id,product_snapshot,quantity,price
    </sql>
</mapper>
